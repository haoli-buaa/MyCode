package Thread;

public class MySingleton{
	private static MySingleton instance = null;
	private MySingleton(){
		
	}
	public synchronized static MySingleton getInstance(){
		if(null == instance){
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			instance = new MySingleton();
		}
		return instance;
	}	
}
